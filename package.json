{
    "name": "universal_app_demo",
    "version": "1.0.0",
    "description": "universal_react_app",
    "scripts": {
        "buildv": "webpack --config build/webpack.vendor.js",
        "buildc": "webpack --config build/webpack.config.js",
        "builds": "webpack --config build/webpack.server.js",
        "build": "rimraf vendor && npm run buildv && rimraf dist && npm run buildc && npm run builds",
        "devc": "rimraf dist && cross-env NODE_ENV=development webpack-dev-server --config build/webpack.config.js",
        "devs": "cross-env NODE_ENV=development nodemon ./server/server.js",
        "start": "cross-env NODE_ENV=production && npm run build && node ./dist/server-app.js",
        "start:nobuild": "cross-env NODE_ENV=production && node ./dist/server-app.js",
        "devt": "node ./dist/server-app.js"
    },
    "keywords": [
        "react",
        "webpack",
        "express",
        "babel",
        "ejs",
        "nodemon"
    ],
    "author": "Melor Zhang",
    "license": "ISC",
    "dependencies": {
        "axios": "^0.18.0",
        "ejs": "^2.6.1",
        "express": "^4.16.3",
        "import-inspector": "^2.0.0",
        "prop-types": "^15.6.1",
        "react": "^16.4.0",
        "react-dom": "^16.4.0",
        "react-fader": "^2.0.3",
        "react-loadable": "^5.4.0",
        "react-redux": "^5.0.7",
        "react-router": "^4.2.0",
        "react-router-dom": "^4.2.2",
        "react-router-transition-switch": "^1.1.0",
        "redux": "^4.0.0"
    },
    "devDependencies": {
        "add-asset-html-webpack-plugin": "^2.1.3",
        "assets-webpack-plugin": "^3.6.0",
        "autoprefixer": "^8.6.0",
        "babel-core": "^6.26.3",
        "babel-loader": "^7.1.4",
        "babel-plugin-import-inspector": "^2.0.0",
        "babel-plugin-syntax-dynamic-import": "^6.18.0",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "babel-preset-env": "^1.7.0",
        "babel-preset-react": "^6.24.1",
        "babel-register": "^6.26.0",
        "clean-webpack-plugin": "^0.1.19",
        "cross-env": "^5.1.6",
        "css-loader": "^0.28.11",
        "cssnano": "^3.10.0",

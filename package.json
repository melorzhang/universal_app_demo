{
  "name": "universal_app_demo",
  "version": "1.0.0",
  "description": "universal_react_app",
  "scripts": {
    "bulidv": "webpack --config build/webpack.vendor.js",
    "buildc": "npm run buildv && webpack --config build/webpack.config.js",
    "builds": "webpack --config build/webpack.server.js",
    "build": "rimraf dist && npm run buildc && npm run builds",
    "devc": "rimraf dist && cross-env NODE_ENV=development webpack-dev-server --config build/webpack.config.js",
    "devs": "npm run devc && cross-env NODE_ENV=development nodemon ./server/server.js",
    "start": "cross-env NODE_ENV=production && npm run build && node ./server/server.js"
  },
  "keywords": [
    "react",
    "webpack",
    "express",
    "babel",
    "ejs",
    "nodemon"
  ],
  "author": "Melor Zhang",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.18.0",
    "ejs": "^2.6.1",
    "express": "^4.16.3",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-loadable": "^5.4.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.6.0",
    "autoprefixer": "^8.6.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "clean-webpack-plugin": "^0.1.19",
    "cross-env": "^5.1.6",
    "css-loader": "^0.28.11",
    "cssnano": "^3.10.0",
    "ejs-compiled-loader": "^1.1.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "http-proxy-middleware": "^0.18.0",
    "less": "^3.0.4",
    "less-loader": "^4.1.0",
    "memory-fs": "^0.4.1",
    "mini-css-extract-plugin": "^0.4.0",
    "nodemon": "^1.17.5",
    "optimize-css-assets-webpack-plugin": "^4.0.2",
    "postcss-loader": "^2.1.5",
    "react-hot-loader": "^4.2.0",
    "rimraf": "^2.6.2",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack": "^4.10.2",
    "webpack-cli": "^3.0.1",
    "webpack-dev-server": "^3.1.4"
  }
}
